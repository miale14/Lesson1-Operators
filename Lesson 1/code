#python
matrix1 = [[1, 2], [3, 4]]
matrix2 = [[5, 6], [7, 8]]
# Initialize a result matrix with zeros
result = [[0, 0], [0, 0]]
# Perform matrix multiplication
for i in range(len(matrix1)):
  for j in range(len(matrix2[0])):
    for k in range(len(matrix2)):
      result[i][j] += matrix1[i][k] * matrix2[k][j]
print(result)
# This will print [[19, 22], [43, 50]]



# Using the and operator
x = 5
print(x > 0 and x < 10)
# True because both conditions are True
# Using the or operator
y = 15
print(y < 10 or y % 2 == 0)
# True because one condition (y % 2 == 0) is True
# Using the not operator
z = False
print(not z)
# True because z is False
 
#python
x = 5 # assignmement operator
x += 2
# Equivalent to x = x + 2, so x will be 7
y = -x # unary operator
